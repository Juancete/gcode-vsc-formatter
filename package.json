{
	"name": "gcode-formatter",
	"displayName": "GCode Formatter",
	"description": "Formats ISO code",
	"version": "0.0.8",
	"publisher": "Juancete",
	"license": "MPL 2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/Juancete/gcode-vsc-formatter"
	},
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"onLanguage:gcode",
		"onLanguage:centroid-gcode"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "GCODE",
				"aliases": [
					"gcode"
				],
				"extensions": [
					"nc",
					"gcode"
				]
			},
			{
				"id": "Centroid-Gcode",
				"aliases": [
					"centroid-gcode"
				],
				"extensions": [
					"nc",
					"gcode"
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run lint && npm run compile",
		"lint": "tslint -p ./",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.17.27",
		"@types/vscode": "^1.47.0",
		"codelyzer": "~5.1.2",
		"glob": "^7.1.6",
		"mocha": "^6.2.3",
		"tslint": "^5.20.1",
		"typescript": "^3.9.7",
		"vscode-test": "^1.4.0"
	}
}
