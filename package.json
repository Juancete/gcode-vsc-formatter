{
	"name": "gcode-formatter",
	"displayName": "GCode Formatter",
	"description": "Formats ISO code",
	"version": "0.0.5",
	"publisher": "Juancete",
	"repository": {
		"type": "git",
		"url": "https://https://github.com/Juancete/gcode-vsc-formatter.git"
	  },
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"onLanguage:gcode",
		"onLanguage:centroid-gcode"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
            {
                "id": "gocde",
                "aliases": [
                    "Foo"
                ],
                "extensions": [
                    "foo"
                ]
            }
        ]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run lint && npm run compile",
		"lint": "tslint -p ./",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.2.0",
		"tslint": "^5.18.0",
		"typescript": "^3.5.3",
		"vscode-test": "^1.2.0"
	}
}
